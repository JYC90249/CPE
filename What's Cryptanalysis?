#include<iostream>
#include <algorithm>

using namespace std;

int main()
{
    int n;
    cin>>n;
    string str, tmp;
    for(int i=0;i<n;i++){
        cin>>tmp;
        str = str+tmp;
    }
    for(int i=0;i<str.length();i++){
        str[i] = toupper(str[i]);
    }
    cout<<str;
    /*下面開始排序*/
}
